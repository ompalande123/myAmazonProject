package base;



import org.testng.annotations.AfterSuite;

import io.cucumber.java.After;
import utilities.DriverFactory;

public class TestBase {
	
	
	
	
	@After
	public void afterScenario()
	{
		DriverFactory.quitDriver();
	}

	@AfterSuite
	public void afterSuite()
	{
		DriverFactory.quitDriver();
	}
	
	
}
